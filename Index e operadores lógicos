# vetores
# chamando a base
ChickWeight

# acessa o primeiro elemento
ChickWeight[1] 

# acessa os elementos 1, 2 até 4
ChickWeight[c(1:2, 4)] 


# matrizes
ChickWeight[1, ] # linha 1
ChickWeight[ ,1] # coluna 1
ChickWeight[1,1] # linha 1, coluna 1

# data.frames
# retorna apenas a coluna weight do data.frame chickweight
ChickWeight$weight 

# retorna todas as linhas apenas a coluna time do data.frame ChickWeight
ChickWeight[ , 3] 

# retorna as 10 primeiras linhas das colunas 2 a 3 do data.frame ChickWeight
ChickWeight[1:10, 2:3] 

# retorna a coluna Time, indexada pelo nome
ChickWeight[, 'Time'] 

# retorna a coluna espécies, indexada pelo nome, em formato de coluna
ChickWeight[, 'weight', drop = FALSE] 

# retorna todas as colunas, menos a 2 (weight)
ChickWeight[ , -1] 
